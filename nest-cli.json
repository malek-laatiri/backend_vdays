{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/orders/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/orders/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/orders",
  "projects": {
    "orders": {
      "type": "application",
      "root": "apps/orders",
      "entryFile": "main",
      "sourceRoot": "apps/orders/src",
      "compilerOptions": {
        "tsConfigPath": "apps/orders/tsconfig.app.json"
      }
    },
    "billing": {
      "type": "application",
      "root": "apps/billing",
      "entryFile": "main",
      "sourceRoot": "apps/billing/src",
      "compilerOptions": {
        "tsConfigPath": "apps/billing/tsconfig.app.json"
      }
    },
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "scrapping": {
      "type": "application",
      "root": "apps/scrapping",
      "entryFile": "main",
      "sourceRoot": "apps/scrapping/src",
      "compilerOptions": {
        "tsConfigPath": "apps/scrapping/tsconfig.app.json"
      }
    },
    "classification": {
      "type": "application",
      "root": "apps/classification",
      "entryFile": "main",
      "sourceRoot": "apps/classification/src",
      "compilerOptions": {
        "tsConfigPath": "apps/classification/tsconfig.app.json"
      }
    },
    "wishlist": {
      "type": "application",
      "root": "apps/wishlist",
      "entryFile": "main",
      "sourceRoot": "apps/wishlist/src",
      "compilerOptions": {
        "tsConfigPath": "apps/wishlist/tsconfig.app.json"
      }
    },
    "history": {
      "type": "application",
      "root": "apps/history",
      "entryFile": "main",
      "sourceRoot": "apps/history/src",
      "compilerOptions": {
        "tsConfigPath": "apps/history/tsconfig.app.json"
      }
    },
    "user": {
      "type": "application",
      "root": "apps/user",
      "entryFile": "main",
      "sourceRoot": "apps/user/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user/tsconfig.app.json"
      }
    },
    "settings": {
      "type": "application",
      "root": "apps/settings",
      "entryFile": "main",
      "sourceRoot": "apps/settings/src",
      "compilerOptions": {
        "tsConfigPath": "apps/settings/tsconfig.app.json"
      }
    }
  }
}